{
    "name": "paquebot",
    "version": "0.1.0",
    "description": "Discord bot in JavaScript",
    "main": "src/index.js",
    "scripts": {
        "build": "babel src -d dist --copy-files",
        "dev": "nodemon --exec babel-node -r dotenv/config src/index.js",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "test": "jest",
        "validate:lint": "eslint src",
        "validate:prettier": "prettier --check ./src/**/*.js",
        "validate": "npm run validate:lint && npm run validate:prettier"
    },
    "dependencies": {
        "@discordjs/rest": "^0.1.0-canary.0",
        "axios": "^0.23.0",
        "discord-api-types": "^0.23.1",
        "discord.js": "^13.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/node": "^7.15.8",
        "@babel/plugin-proposal-throw-expressions": "^7.14.5",
        "@babel/preset-env": "^7.15.8",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-node": "^4.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "nodemon": "^2.0.13",
        "prettier": "^2.4.1",
        "undefined": "^0.1.0"
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}
